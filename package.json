{
	"name": "jakobpusterhofer.dev",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev ",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"optimize-images": "image-transmutation --run --sourceFolder './build/images' --targetFolder './build/images' --inputFormats 'jpg' --inputFormats 'jpeg' --inputFormats 'png' --outputFormats 'png' --outputFormats 'webp' --outputFormats 'avif'",
		"postbuild": "npm run optimize-images && svelte-sitemap --domain https://jakobpusterhofer.dev/",
		"vercel-sitemap": "svelte-sitemap --out-dir .vercel/output/static --domain https://jakobpusterhofer.dev/",
		"vercel-optimize-images": "image-transmutation --run --sourceFolder '.vercel/output/static/images' --targetFolder '.vercel/output/static/images' --inputFormats 'jpg' --inputFormats 'jpeg' --inputFormats 'png' --outputFormats 'png' --outputFormats 'webp' --outputFormats 'avif'",
		"vercel-build": "npm run build && npm run vercel-sitemap && npm run vercel-optimize-images"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.0.0",
		"@types/dateformat": "^5.0.0",
		"dateformat": "^5.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"image-transmutation": "^0.1.2",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.57.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-sitemap": "^2.5.4",
		"typescript": "^4.9.3",
		"vite": "^4.1.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/lato": "^5.0.17",
		"@fontsource/league-spartan": "^5.0.12",
		"@fontsource/nunito-sans": "^5.0.8",
		"@fontsource/poppins": "^5.0.8",
		"@fontsource/ubuntu": "^5.0.8",
		"@fontsource/ubuntu-mono": "^4.5.11",
		"@sanity/client": "^6.7.1",
		"@sanity/image-url": "^1.0.2",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@vercel/analytics": "^1.1.1",
		"embla-carousel": "^8.0.0-rc14",
		"embla-carousel-autoplay": "^8.0.0-rc14",
		"embla-carousel-svelte": "^8.0.0-rc14",
		"svelte-embla": "^0.0.7",
		"svelte-modals": "^1.3.0",
		"vanilla-cookieconsent": "^3.0.0-rc.16"
	}
}
